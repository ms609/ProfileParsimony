	tree tree21 = [&U]   \item{iter}{iteration number of calling function, for reporting to user only; }
	tree tree22 = [&U]   \item{cluster}{a cluster, prepared with \code{\link{prepare.cluster}}, to accelerate;
	tree tree23 = [&U]     searches on multicore machines; }
	tree tree24 = [&U]   \item{trace}{determines how much information to output to screen.}
	}
	tree tree26 = [&U] \value{This function returns the most parsimonious of the trees generated, with attributes \code{hits} and \code{pscore}
	tree tree27 = [&U]  as described for argument \code{tree}.}
	tree tree28 = [&U] \author{Martin R. Smith}
	tree tree29 = [&U] \seealso{
	tree tree30 = [&U]   \itemize{
	tree tree31 = [&U]     \item \code{\link{rooted.nni}}
	tree tree32 = [&U]     \item \code{\link{rooted.spr}}
	tree tree33 = [&U]     \item \code{\link{rooted.tbr}}
	  }
	}
	tree tree36 = [&U] \examples{
	tree tree37 = [&U] data('SigSut')
	tree tree38 = [&U] random.tree <- rtree(34, tip.label[&Annot="names(SigSut.data),"] br[&Annot="NULL)"]
	tree tree39 = [&U] rearrange.tree(random.tree, SigSut.preparedata, rooted.nni)

end;