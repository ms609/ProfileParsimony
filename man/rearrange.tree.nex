begin taxa;
	dimensions ntax=0;
	taxlabels
	;
end;
begin trees;
	tree tree1 = [&U] \alias{rearrange.tree}
	tree tree2 = [&U] \title{Rearrange phylogenetic tree}
	tree tree3 = [&U] \description{Performs one tree rearrangement of the specified type.}
	tree tree4 = [&U] \usage{
	tree tree5 = [&U] rearrange.tree(tree, data, rearrange, min.score = NULL, concavity = NULL, return.single = TRUE,
	tree tree6 = [&U]  iter = "<unknown>", cluster = NULL, trace = 0)
	}
	tree tree8 = [&U] \arguments{
	tree tree9 = [&U]   \item{tree}{a rooted bifurcating phylogenetic tree with the desired outgroup, and the attributes
	tree tree10 = [&U]     \code{pscore}, the tree's parsimony score, and;
	tree tree11 = [&U]     \code{hits}, the number of times the best score has been hit in the calling function;
	  }
	tree tree13 = [&U]   \item{data}{a data matrix defined by \code{\link{prepare.data}}; }
	tree tree14 = [&U]   \item{rearrange}{a rearrangement function: probably one of;
	tree tree15 = [&U]     \code{\link{rooted.nni}}, \code{\link{rooted.spr}} or \code{\link{rooted.tbr}}; }
	tree tree16 = [&U]   \item{min.score}{trees longer than \code{min.score}, probably the score of the starting tree,
	tree tree17 = [&U]     will be discarded; }
	tree tree18 = [&U]   \item{concavity}{concavity constant for implied weighting (not currently implemented!);
	tree tree19 = [&U]     see \code{\link{parsimony.inapp}}; }
